
/******************************************************************************
 *  Compilation:  javac -d bin RangePrim.java
 *  Execution:    java -cp bin com.bridgelabz.util.RangePrim
 *  
 *  Purpose: Determines whether entered number  n is prime.
 *
 *  @author  ragini patil
 *  @version 1.0
 *  @since   06-08-2017
 ******************************************************************************/

package com.bridgelabz.util;
public class RangePrime{
	static int num1,num2;
	//static int num2;
	public static void main(String args[]){
		num1=Integer.parseInt(args[0]);
		num2=Integer.parseInt(args[1]);
		System.out.println("entered range is="+num1+" "+num2);	
			for(int i=num1;i<num2;i++){
				/*check condition if number is having remainder ==0
				if number is divisible&remainder=0 hence it is non-prime*/
				for(int j=2;j<(i);j++){
					if(i%j==0){
						break;
						//System.out.println(" not prime");
					}
					else{									
						System.out.println(" "+i);
						break;
					}.
				
				}	//end of j for loop	
				
			}		//end of i for loop				
	}
}
		
			
		

